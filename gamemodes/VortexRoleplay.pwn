		format(string, sizeof(string), "%s has rewarded %s $%s for %s.", Player[playerid][AdminName], GetName(id), IntToFormattedStr(amount), reason);
		AdminActionsLog(string);
		Player[id][Money] += amount;
		SavePlayerData(id);
	}

	return 1;
}


