		return SendClientMessage(playerid, -1, "You don't have a vehicle that you can register.");

	ShowPlayerDialog(playerid, REGISTER_VEHICLE, DIALOG_STYLE_LIST, "Vehicle Registration", res, "Register", "Cancel");
	return 1;
