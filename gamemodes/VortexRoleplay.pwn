		        Player[playerid][Wep5Pos][1] = fOffsetY;
		        Player[playerid][Wep5Pos][2] = fOffsetZ;
		        Player[playerid][Wep5Pos][3] = fRotX;
		        Player[playerid][Wep5Pos][4] = fRotY;
		        Player[playerid][Wep5Pos][5] = fRotZ;
