	//if((Player[playerid][Job] != Veh[idx][Job] && Player[playerid][Job2] != Veh[idx][Job]) || (Veh[idx][Job] == 8 && (Player[playerid][PizzaCooldown] > gettime() && Player[playerid][PizzaRun] == 0)))
