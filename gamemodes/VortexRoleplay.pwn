		        Player[playerid][Wep6Pos][1] = fOffsetY;
		        Player[playerid][Wep6Pos][2] = fOffsetZ;
		        Player[playerid][Wep6Pos][3] = fRotX;
		        Player[playerid][Wep6Pos][4] = fRotY;
		        Player[playerid][Wep6Pos][5] = fRotZ;
