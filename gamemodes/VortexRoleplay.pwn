stock StripNewLine(string[])
{
	new len = strlen(string);
	if (string[0]==0)
		return ;
	if ((string[len - 1] == '\n') || (string[len - 1] == '\r'))
	{
		string[len - 1] = 0;
		if (string[0]==0)
			return ;
		if ((string[len - 2] == '\n') || (string[len - 2] == '\r'))
			string[len - 2] = 0;
	}
}

