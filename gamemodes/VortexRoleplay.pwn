		{
			Player[playerid][Job] = 0;
		}

		if(Jobs[Player[playerid][Job2]][JobType] != JOB_MECHANIC && Jobs[Player[playerid][Job2]][JobType] != JOB_DETECTIVE)
		{
			Player[playerid][Job2] = 0;
		}
	}

