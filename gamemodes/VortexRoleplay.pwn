			// if(!IsABike(VehID) && engine != 1 && Veh[idx][Fuel] > 0 && strcmp(Veh[idx][Owner], GetName(playerid), false) && Player[playerid][TempKey] != sql && Player[playerid][PlayingHours] < 10)
