stock DetachWeaponFromPlayer(playerid, weaponid)
{
	for(new i; i < 10; i++)
	{
		if(Player[playerid][HolsteredWeapon][i] == weaponid)
		{
			RemovePlayerAttachedObject(playerid, i);
			Player[playerid][HolsteredWeapon][i] = 0;
		}
	}
	return 1;
}

