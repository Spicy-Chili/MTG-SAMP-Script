			ShowPlayerDialog(playerid, 9843, DIALOG_STYLE_INPUT, "Authentication (Admin PIN)", "Please enter your desired PIN.", "Done", "Cancel");
		}
		else
		{
			Player[playerid][PINLoginTimer] = defer AdminPINLogin(playerid); 
