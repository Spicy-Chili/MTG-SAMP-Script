		PlayerTextDrawHide(playerid, CarPreviewText[playerid]);
		for(new i; i < 4; i++)
			TextDrawHideForPlayer(playerid, CarPreview[i]);
		CancelSelectTextDraw(playerid);
		DeletePVar(playerid, "CCV");
