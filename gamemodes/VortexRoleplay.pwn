				ShowPlayerDialog(playerid, 6897, DIALOG_STYLE_LIST, "Report System - PLEASE ONLY REPORT IF YOUR REPORT IS URGENT!", "Report a Player\nSpeak to an Administrator\nReport Masked Player", "Proceed", "Cancel");
