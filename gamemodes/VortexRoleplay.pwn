	{
		if(Player[i][BeingDraggedBy] == id)
		{
			format(string, sizeof(string), "* %s has stopped dragging %s.", GetNameEx(id), GetNameEx(i));
			NearByMessage(id, NICESKY, string);
			Player[i][BeingDraggedBy] = -1;
			
