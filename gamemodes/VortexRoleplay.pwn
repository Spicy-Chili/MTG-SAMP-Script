	Veh[idx][Mod9] = 0;
	Veh[idx][Mod10] = 0;
	Veh[idx][Mod11] = 0;
	Veh[idx][Mod12] = 0;
	Veh[idx][Mod13] = 0;

	DespawnVehicleSQL(sql);
