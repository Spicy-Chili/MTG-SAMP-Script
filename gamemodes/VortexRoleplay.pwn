	else SendClientMessage(playerid, -1, "You've purchased a vehicle!");

	SavePlayerData(playerid);
	format(string, sizeof(string), "%s has purchased a %d for %d.", GetName(playerid), carid, price);
	StatLog(string);

	return 1;
