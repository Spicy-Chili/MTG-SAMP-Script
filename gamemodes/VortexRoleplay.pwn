stock GetEmptyBackSeat(veh)
{
	new seat1, seat2;
	foreach(Player, p)
	{
		if(!IsPlayerInAnyVehicle(p))
			continue;

		if(IsPlayerInVehicle(p, veh))
		{
			if(GetPlayerVehicleSeat(p) == 2)
				seat1++;
			else if(GetPlayerVehicleSeat(p) == 3)
				seat2++;
		}
	}
	if(seat1 == 0)
		return 2;
	else if(seat2 == 0)
		return 3;

	return -1;
}

