				strcat(password, salt, sizeof(password));
				WP_Hash(buff, sizeof(buff), password);

				mysql_format(MYSQL_MAIN, query, sizeof(query), "UPDATE playeraccounts SET Password = '%e', pSalt = '%e' WHERE NormalName = '%e'", buff, salt, name);
