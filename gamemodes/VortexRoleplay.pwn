	new url[255];
	format(url, sizeof(url), "http://translate.google.com/translate_tts?tl=en&q=%s", msg);

	StopAudioStreamForPlayer(id);
	PlayAudioStreamForPlayer(id, url);
	SetPVarInt(playerid, "TTS_AntiSpam", gettime() + 5);
	return 1;
}

